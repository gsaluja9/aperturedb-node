{
  "name": "aperturedb",
  "version": "0.0.1",
  "description": "Node.js SDK for ApertureDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./base": {
      "types": "./dist/base.d.ts",
      "import": "./dist/base.js"
    },
    "./image": {
      "types": "./dist/image.d.ts",
      "import": "./dist/image.js"
    },
    "./descriptor": {
      "types": "./dist/descriptor.d.ts",
      "import": "./dist/descriptor.js"
    },
    "./descriptor_set": {
      "types": "./dist/descriptor_set.d.ts",
      "import": "./dist/descriptor_set.js"
    },
    "./polygon": {
      "types": "./dist/polygon.d.ts",
      "import": "./dist/polygon.js"
    },
    "./bbox": {
      "types": "./dist/bbox.d.ts",
      "import": "./dist/bbox.js"
    },
    "./video": {
      "types": "./dist/video.d.ts",
      "import": "./dist/video.js"
    },
    "./frame": {
      "types": "./dist/frame.d.ts",
      "import": "./dist/frame.js"
    },
    "./clip": {
      "types": "./dist/clip.d.ts",
      "import": "./dist/clip.js"
    },
    "./entity": {
      "types": "./dist/entity.d.ts",
      "import": "./dist/entity.js"
    },
    "./connection": {
      "types": "./dist/connection.d.ts",
      "import": "./dist/connection.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    }
  },
  "scripts": {
    "build": "tsc && mkdir -p dist/proto && cp src/proto/*.proto dist/proto/",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --watchAll",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "src/proto/*.proto"
  ],
  "keywords": [
    "aperturedb",
    "database",
    "sdk",
    "typescript",
    "nodejs"
  ],
  "author": "CoffeeBlack AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gsaluja9/aperturedb-node.git"
  },
  "bugs": {
    "url": "https://github.com/gsaluja9/aperturedb-node/issues"
  },
  "homepage": "https://github.com/gsaluja9/aperturedb-node#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "async-lock": "^1.4.1",
    "dotenv": "^16.3.1",
    "protobufjs": "^7.4.0",
    "python-struct": "^1.1.3"
  },
  "devDependencies": {
    "@types/async-lock": "^1.4.2",
    "@types/jest": "^29.5.11",
    "@types/python-struct": "^1.0.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
